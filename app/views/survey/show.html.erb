<div class="Survey list">
<h2>Questions (sorted by title)</h2>



	<h4><%=link_to("Add a Question", {:controller=>'question', :action =>'new', :id => @survey.id})%></h4>

<table class="listing" summary="Questions List">
 <tr class="header">
  <th>ID</th>
  <th>Questions</th>
  <th>Action</th>
</tr>
<% @survey.questions.each do |t| %>
<tr>
	<td><%=t.id %></td>
	<td ><%= t.title %></td>
	
	<td class="actions" >
		<%= link_to("Edit",  {:controller => 'question', :action =>'edit', :id => t.id}, :class => 'action edit')   %>
		<%= link_to("Delete", {:controller => 'question', :action =>'delete', :id => t.id}, :class => 'action delete')  %>
	</td>
 
	
	 
</tr>

<% end %>


</table>
</div>
