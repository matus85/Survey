
<div class="Survey_list">
<h2>Take A Survey  </h2>

<table class="center" class="listing" summary="Movie List">
 <tr class="header">
  <th>ID</th>
  <th>Title</th>
  <th>Action</th>
</tr>
<% @surveys.each do |t| %>
<tr>
	<td><%=t.id %></td>
	<td ><%= t.title %></td>
	
	<td class="actions" >
		<%= link_to("Take", {:action =>'takeSurvey', :id => t.id}, :class => 'action takeSurvey')%>
	</td>
 
	
	 
</tr>

<% end %>


</table>
</div>

<br>
