
<div class="Survey list">
<h2>Surveys  </h2>
 

<table class="center listing" summary="Movie List">
 <tr class="header">
  <th>ID</th>
  <th>Title</th>
  <th>Action</th>
</tr>
<% @surveys.each do |t| %>
<tr>
	<td><%=t.id %></td>
	<td ><%= t.title %></td>
	
	<td class="actions" >
		<%= link_to("View Result", {:action =>'viewResult', :id => t.id}, :class => 'action viewResult')%>
	</td>
 
	
	 
</tr>

<% end %>


</table>
</div>
