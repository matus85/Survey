
<div class="Question list">
<h2>Question (sorted by title)</h2>



	<h4><%=link_to("Create New Question", {:action =>'new'})%></h4>

<table class="listing" summary="Question List">
 <tr class="header">
  <th>ID</th>
  <th>Title</th>
  <th>Associated with Survey</th>
  <th>Action</th>
</tr>
<% @questions.each do |t| %>
<tr>
	<td><%=t.id %></td>
	<td ><%= t.title %></td>
	<td ><%= t.survey.title %></td>
	
	<td class="actions" >
		<%= link_to("Show", {:action =>'show', :id => t.id}, :class => 'action show')%>
		<%= link_to("Edit",  {:action =>'edit', :id => t.id}, :class => 'action edit')   %>
		<%= link_to("Delete", {:action =>'delete', :id => t.id}, :class => 'action delete')  %>
	</td>
 
	
	 
</tr>

<% end %>


</table>
</div>
